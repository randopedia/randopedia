<div class="main-container container-fluid">

    <div class="main-view">

        <div class="row">
            <div class="col-sm-12 col-md-6">
                <div class="panel">
                    <h3>
                        Last updated tours
                    </h3>
                    {{lastupdated-component store=store}}
                </div>

            </div>

            <div class="col-sm-12 col-md-6">
                <h3>
                    Stats
                </h3>
                {{stat-component stat=stat}}
            </div>
        </div>

        <div class="row">
            <div class="col-sm-12">
                {{tagcloud-component tags=tags}}
            </div>
        </div>

        <div class="row">
            <div class="col-sm-12">
                <h3>
                    #<span class="logo-font">randopedia</span> on Instagram
                </h3>
                {{instafeed-component tagName='randopedia'}}
            </div>
        </div>

    </div>
</div>